{
  "openapi": "3.0.1",
  "info": {
    "title": "TreasureHunt.Api",
    "version": "1.0"
  },
  "paths": {
    "/api/TreasureHunt/solve": {
      "post": {
        "tags": [
          "TreasureHunt"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TreasureHuntInput"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/TreasureHuntInput"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/TreasureHuntInput"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/TreasureHuntResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TreasureHuntResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/TreasureHuntResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/TreasureHunt/history": {
      "get": {
        "tags": [
          "TreasureHunt"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": { }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": { }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": { }
                }
              }
            }
          }
        }
      }
    },
    "/api/TreasureHunt/{id}": {
      "get": {
        "tags": [
          "TreasureHunt"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Position": {
        "type": "object",
        "properties": {
          "row": {
            "type": "integer",
            "format": "int32"
          },
          "col": {
            "type": "integer",
            "format": "int32"
          },
          "chestNumber": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "TreasureHuntInput": {
        "required": [
          "m",
          "matrix",
          "n",
          "p"
        ],
        "type": "object",
        "properties": {
          "n": {
            "maximum": 500,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "m": {
            "maximum": 500,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "p": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "matrix": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "integer",
                "format": "int32"
              }
            }
          }
        },
        "additionalProperties": false
      },
      "TreasureHuntResponse": {
        "type": "object",
        "properties": {
          "minFuel": {
            "type": "number",
            "format": "double"
          },
          "path": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Position"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}